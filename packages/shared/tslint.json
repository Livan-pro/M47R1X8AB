{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "member-access": false,
    "member-ordering": [
      true,
      "fields-first"
    ],
    "ordered-imports": false,
    "max-line-length": [
      true,
      150
    ],
    "curly": [
      true,
      "ignore-same-line"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "object-literal-sort-keys": false,
    "no-unused-expression": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format"
    ]
  },
  "rulesDirectory": []
}