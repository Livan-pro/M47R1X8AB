{
  "name": "webadmin",
  "version": "1.1.0",
  "description": "Matrix's web admin application",
  "author": "Livan",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "codegen": "apollo codegen:generate --target typescript --queries \"gql/*.ts\" --outputFlat gql/__generated__/",
    "deploy": "cp -r ./dist /var/www/webadmin-new && rm -rf /var/www/webadmin-old && mv /var/www/webadmin /var/www/webadmin-old || true && mv /var/www/webadmin-new /var/www/webadmin"
  },
  "dependencies": {
    "@nuxtjs/apollo": "^4.0.0-rc9",
    "graphql-tag": "^2.10.1",
    "nuxt": "^2.0.0",
    "nuxt-property-decorator": "^2.3.0",
    "shared": "^1.1.0",
    "vue-meta": "^2.2.2",
    "vue-qr": "^2.1.0",
    "vuetify": "^2.0.18"
  },
  "devDependencies": {
    "@nuxt/typescript": "^2.8.1",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^0.2.1",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/vuetify": "^1.0.0",
    "@types/node": "^12.6.9",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "apollo": "^2.16.3",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "sass": "^1.22.9",
    "sass-loader": "^7.1.0",
    "ts-node": "^8.3.0"
  }
}
